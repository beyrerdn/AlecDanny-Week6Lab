<div class="row">
  <div class="welcome col-xs-10 col-xs-offset-2">
    <h1>Meeper</h1>
      <% if !user_signed_in? %>
      <ul>
        <li><%= link_to "Sign up", new_user_registration_path %></li>
        <li><%= link_to "Login", new_user_session_path %></li>
      </ul>
      <% else%>
      <ul>
        <li><%= link_to "Your profile", user_page_path(:username => current_user.username) %></li>
      </ul>
      <% end %>
  </div>
  </div>
